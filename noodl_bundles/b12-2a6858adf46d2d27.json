n    NewPassword: \"string\",\r\n    ConfirmPassword: \"string\"\r\n}\r\n\r\nScript.Outputs = {\r\n    ToastType: \"string\",\r\n    ToastMsg: \"string\",\r\n    ToastShow: \"signal\",\r\n    SetPass: \"signal\"\r\n}\r\n\r\nScript.Signals.Run = function () {\r\n\r\n    function checkPassword(str)\r\n    {\r\n    var re = /^(?=.*\\d)(?=.*[!@#$%^&*])(?=.*[a-z])(?=.*[A-Z]).{8,}$/;\r\n    return re.test(str);\r\n    }\r\n\r\n    // Is the new password and confirm password the same.\r\n    const newPass = Script.Inputs.NewPassword;\r\n    const confirmPass = Script.Inputs.ConfirmPassword;\r\n    if (newPass != confirmPass){\r\n        Script.Outputs.ToastType = \"Error\";\r\n        Script.Outputs.ToastMsg = \"New passwords do not match.\";\r\n        Script.Outputs.ToastShow();\r\n    }\r\n    // Then is the new password valid (characters / types).\r\n    else if (!checkPassword(newPass)){\r\n    console.log(2)\r\n        Script.Outputs.ToastType = \"Error\";\r\n        Script.Outputs.ToastMsg = \"Password does not meet the minimum criteria.\"\r\n        Script.Outputs.ToastShow();\r\n    }\r\n    else{\r\n        Script.Outputs.SetPass();\r\n    }\r\n}"},"ports":[{"name":"CurrentPassword","plug":"input","type":{"name":"string"},"group":"Inputs","index":5},{"name":"NewPassword","plug":"input","type":{"name":"string"},"group":"Inputs","index":6},{"name":"ConfirmPassword","plug":"input","type":{"name":"string"},"group":"Inputs","index":7},{"name":"Run","plug":"input","type":{"name":"signal"},"group":"Inputs","index":8},{"name":"ToastType","type":{"name":"string"},"plug":"output","group":"Outputs","index":9},{"name":"ToastMsg","type":{"name":"string"},"plug":"output","group":"Outputs","index":10},{"name":"ToastShow","type":{"name":"signal"},"plug":"output","group":"Outputs","index":11},{"name":"SetPass","type":{"name":"signal"},"plug":"output","group":"Outputs","index":12}],"children":[]},{"id":"2360a418-7dd2-f3af-9ca0-420a4f214b7d","type":"/Visual Comps/Show Toast","param
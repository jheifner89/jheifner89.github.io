 false};\r\n    }\r\n\r\n    var filteredData = []\r\n    var temp = []\r\n    var siteIdFilter = Script.Inputs.SiteIDFilter\r\n    // Filter By Site ID\r\n    if (siteIdFilter != null || siteIdFilter != \"\"){\r\n        for (const tkt in data){\r\n            if(data[tkt]['site_id'].toLowerCase().startsWith(Script.Inputs.SiteIDFilter.toLowerCase())){\r\n                temp.push(data[tkt])\r\n            };\r\n        };\r\n    }\r\n    else{\r\n        temp = data\r\n    };\r\n    filteredData = temp;\r\n    temp = []\r\n    \r\n    var priorityFilter = Script.Inputs.PriorityFilter\r\n    if (priorityFilter == \"All\"){priorityFilter = null}\r\n    if (priorityFilter != null){\r\n        for (const tkt in filteredData){\r\n            if(filteredData[tkt]['priority'] == Script.Inputs.PriorityFilter){\r\n                temp.push(filteredData[tkt])\r\n            }\r\n        }\r\n    }    \r\n    else {\r\n        temp = filteredData\r\n    };\r\n    filteredData = temp;\r\n    temp =[]\r\n\r\n    var statusFilter = Script.Inputs.StatusFilter\r\n    if (statusFilter == \"All\"){statusFilter = null}\r\n    if (statusFilter != null){\r\n        for (const tkt in filteredData){\r\n            if(statusFilter == \"Closed\" && filteredData[tkt]['closed_date'] != null){\r\n                temp.push(filteredData[tkt])   \r\n            }\r\n            else if(statusFilter == \"Resolved\" && filteredData[tkt]['resolved_date'] != null && filteredData[tkt]['closed_date'] == null){\r\n                temp.push(filteredData[tkt])\r\n            }\r\n            else if (statusFilter == \"Open\" && filteredData[tkt]['resolved_date'] == null && filteredData[tkt]['closed_date'] == null){\r\n                temp.push(filteredData[tkt])\r\n            }\r\n            else if (statusFilter == \"New\" && filteredData[tkt]['read'] == false && filteredData[tkt]['resolved_date'] == null && filteredData[tkt]['closed_date'] == null){\r\n                temp.push(filteredData[tkt])\r\n